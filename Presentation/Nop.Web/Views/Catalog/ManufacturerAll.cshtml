@model IList<ManufacturerModel>
<div class="row footer-col parner" style="width:100%">
    <div class="active"><a href="#">@T("Partners")</a></div>
    <ul id="partnerlist">
        @foreach (var item in Model)
        {
            <li class="col-sm-2 logoPartner">
                <a href="@(string.IsNullOrWhiteSpace(item.Url) ? "#" : item.Url)" target="_blank">
                    <img alt="@Html.Raw(HttpUtility.HtmlDecode(item.Name))" src="@item.PictureModel.FullSizeImageUrl" title="@Html.Raw(HttpUtility.HtmlDecode(item.PictureModel.Title))" />
                    
                </a>
            </li>
        }
    </ul>
</div>
@if (Model.Count > 6)
    {
<script type="text/javascript">
    $(document).ready(function () {
        $("#partnerlist").carouFredSel({
            items: 6,
            height: 'auto',
            scroll: {
                items: 1,
                pauseOnHover: true
            }
        });
    });

</script>
    }